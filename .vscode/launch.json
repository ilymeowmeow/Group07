{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build cpp (all src)",
      "type": "shell",
      "command": "g++",
      "args": [
        "-std=c++17",
        "-Wall",
        "-Wextra",
        "-g",
        "src/main.cpp",
        "src/AuthService.cpp",
        "src/adminmodule.cpp",
        "src/StudentView.cpp",
        "src/Teacher.cpp",
        "-o",
        "src/output/main.exe"
      ],
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ],
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Users/ADMIN/Group07/Group07",
      "program": "c:/Users/ADMIN/Group07/Group07/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}